  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/util/",
      "/src/config/",
      "app.controller.ts",
      "app.entity.ts",
      "app.module.ts",
      "app.service.ts",
      "main.ts",
      "google.strategy.ts",
      "/src/auth/",
      "/src/token/",
      "/src/health/"
